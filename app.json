{
  "expo": {
    "name": "PromptPic",
    "slug": "promptpic-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "promptpic",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "sdkVersion": "54.0.0",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "platforms": ["ios", "android"],
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PromptPic to access your camera to take photos for daily prompts."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}


